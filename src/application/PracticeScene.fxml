<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXProgressBar?>
<?import com.jfoenix.controls.JFXSlider?>
<?import com.jfoenix.controls.JFXTreeView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PracticeSceneController">
   <children>
      <Label fx:id="_currentName" alignment="CENTER" layoutX="68.0" layoutY="72.0" prefHeight="112.0" prefWidth="334.0" text="Name">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <JFXButton fx:id="_playAllButton" buttonType="RAISED" layoutX="107.0" layoutY="171.0" onAction="#playAllClicked" prefHeight="84.0" prefWidth="255.0" text="Play">
         <font>
            <Font size="36.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="_recordButton" buttonType="RAISED" layoutX="549.0" layoutY="72.0" onAction="#recordClicked" prefHeight="78.0" prefWidth="242.0" text="Record">
         <font>
            <Font size="36.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="_playRecordingButton" buttonType="RAISED" layoutX="536.0" layoutY="512.0" onAction="#playRecordingClicked" prefHeight="36.0" prefWidth="119.0" text="Play Recording">
         <font>
            <Font size="14.0" />
         </font></JFXButton>
      <JFXButton fx:id="_saveButton" buttonType="RAISED" layoutX="670.0" layoutY="512.0" onAction="#saveClicked" prefHeight="36.0" prefWidth="69.0" text="Save">
         <font>
            <Font size="14.0" />
         </font></JFXButton>
      <JFXSlider fx:id="_volumeSlider" blockIncrement="1.0" layoutX="151.0" layoutY="279.0" onDragDetected="#volumeSliderDragged" prefHeight="14.0" prefWidth="168.0" value="30.0" />
      <JFXSlider fx:id="_micSensitivitySlider" layoutX="151.0" layoutY="448.0" onDragDetected="#micSensitivityDragged" prefHeight="14.0" prefWidth="168.0" value="40.0" />
      <JFXButton fx:id="_prevButton" buttonType="RAISED" layoutX="55.0" layoutY="195.0" onAction="#prevClicked" prefHeight="36.0" prefWidth="85.0" text="Previous">
         <font>
            <Font size="14.0" />
         </font></JFXButton>
      <Label layoutX="43.0" layoutY="388.0" prefHeight="36.0" prefWidth="382.0" text="Microphone Volume and sensitivity">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <JFXButton fx:id="_nextButton" buttonType="RAISED" layoutX="319.0" layoutY="189.0" onAction="#nextClicked" prefHeight="47.0" prefWidth="99.0" text="Next">
         <font>
            <Font size="14.0" />
         </font>
      </JFXButton>
      <JFXProgressBar fx:id="_micSensitivityBar" layoutX="134.0" layoutY="526.0" />
      <JFXTreeView fx:id="_recordinglist" layoutX="549.0" layoutY="150.0" prefHeight="340.0" prefWidth="241.0" />
      <JFXButton fx:id="_deleteButton" buttonType="RAISED" layoutX="740.0" layoutY="512.0" onAction="#deleteClicked" prefHeight="36.0" prefWidth="69.0" text="Delete">
         <font>
            <Font size="14.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
