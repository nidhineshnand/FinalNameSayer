            <Font size="15.0" />
         </font>
         <tooltip>
            <Tooltip text="Your current mic sensitvity.">
               <font>
                  <Font size="18.0" />
               </font>
            </Tooltip>
         </tooltip>
      </Label>
      <JFXButton id="_label" fx:id="_loop" layoutX="466.0" layoutY="20.0" onAction="#loopFiles" prefHeight="36.0" prefWidth="119.0" text="Loop">
         <font>
            <Font size="23.0" />
         </font>
      </JFXButton>
      <Spinner fx:id="_loopCount" layoutX="573.0" layoutY="30.0" prefHeight="26.0" prefWidth="66.0" />
