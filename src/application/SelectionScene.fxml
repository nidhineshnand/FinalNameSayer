<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTabPane?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SelectionSceneController">
   <left>
      <AnchorPane prefHeight="600.0" prefWidth="199.0" BorderPane.alignment="CENTER">
         <children>
            <JFXButton fx:id="_practiceListButton" buttonType="RAISED" layoutX="2.0" layoutY="556.0" onAction="#practiceListClicked" prefHeight="49.0" prefWidth="199.0" text="Practice All" />
            <JFXTreeView fx:id="_practiceList" layoutX="1.0" prefHeight="555.0" prefWidth="200.0" />
            <JFXTabPane layoutX="1.0" prefHeight="554.0" prefWidth="198.0">
               <tabs>
                  <Tab text="Database ">
                     <content>
                        <Pane fx:id="_databaseListPane" prefHeight="200.0" prefWidth="200.0" />
                     </content>
                  </Tab>
                  <Tab text="Custom List">
                     <content>
                        <Pane fx:id="_customListPane" prefHeight="200.0" prefWidth="200.0" />
                     </content>
                  </Tab>
               </tabs>
            </JFXTabPane>
         </children>
      </AnchorPane>
   </left>
   <center>
      <AnchorPane prefHeight="600.0" prefWidth="735.0" BorderPane.alignment="CENTER">
         <children>
            <JFXTextField fx:id="_nameTextField" layoutX="126.0" layoutY="169.0" onAction="#searching" prefHeight="68.0" prefWidth="408.0" promptText="John Doe">
               <font>
                  <Font size="24.0" />
               </font></JFXTextField>
            <Label layoutX="126.0" layoutY="64.0" prefHeight="70.0" prefWidth="350.0" text="Type in a Name">
               <font>
                  <Font size="48.0" />
               </font>
            </Label>
            <JFXButton fx:id="_addToListButton" buttonType="RAISED" layoutX="330.0" layoutY="370.0" onAction="#addToListClicked" prefHeight="49.0" prefWidth="110.0" text="Add to List">
               <font>
                  <Font size="18.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="_uploadButton" buttonType="RAISED" layoutX="126.0" layoutY="474.0" onAction="#uploadClicked" prefHeight="76.0" prefWidth="150.0" text="Upload">
               <font>
                  <Font size="18.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="_practiceButton" buttonType="RAISED" layoutX="440.0" layoutY="370.0" onAction="#practiceClicked" prefHeight="49.0" prefWidth="110.0" text="Practice">
               <font>
                  <Font size="18.0" />
               </font>
            </JFXButton>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
